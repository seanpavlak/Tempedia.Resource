   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "for image in images:\n",
    "    gif = image.split(\".gif\")\n",
    "    img = Image.open(image)\n",
    "    img.save(gif[0]+\".png\",'png', optimize=True, quality=100)"
